{
  "manifest_version": 2,
  "name": "中国数字时代 (CDT-Chinese)",
  "short_name": "CDTChinese",
  "version": "0.0.6",
  
  "description": "免翻墙访问中国数字时代中文网站",
  "icons": {
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  },
  
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://swx.cdn.skype.com/*",
    "*://az844805.vo.msecnd.net/*"
  ],
    
  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icons/icon32.png",
    "default_title": "中国数字时代 (CDT-Chinese)"
  }
}
